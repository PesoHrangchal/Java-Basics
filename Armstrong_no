import java.util.*;
public class Armstrong_no {
	public static void main(String args[]) {
		Scanner s=new Scanner(System.in);
		System.out.print("Enter a number:");
		int n=s.nextInt();
		int number=n;
		int sum=0;
		while(n>0) {
		int temp=n%10;
			sum+=temp*temp*temp;
			n=n/10;
		}
		if(number==sum)
			System.out.print("Armstrong Number");
		else
			System.out.print("Not Armstrong number");
	}
}
